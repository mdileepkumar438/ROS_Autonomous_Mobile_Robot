<?xml version="1.0" ?>
  <robot name="exploDollyrer">


  <link name="base_footprint">
    <visual>
      <geometry>
          <box size="0.35 0.2 0.05"/>
      </geometry>
      <material name="gray">
        <color rgba=".2 .2 .2 1" />
      </material>
    </visual>
    <inertial>
            <mass value="1" />
            <inertia ixx="0.01" ixy="0.0" ixz="0" 
            iyy="0.01" iyz="0" izz="0.01" />
    </inertial>
    <collision>
      <geometry>
        <box size="0.35 0.2 0.05"/>
    </geometry>
      
    </collision>
  </link>

  <link name="wheel_right_link">
    <inertial>
            <mass value="2" />
            <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
    </inertial>

    <visual>
      <geometry>
          <cylinder radius="0.075" length="0.05"/>
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.075" length="0.05"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>
      </surface>
      <contact_coefficients mu="1" kp="1e+13" kd="1.0"/>
    </collision>
  </link>

  <joint name="wheel_right_joint" type="continuous">
    <origin xyz="0.1 0.125 0.0" rpy="1.57 0.0 0.0"/>
    <parent link="base_footprint"/>
    <child link="wheel_right_link"/>
    <axis xyz="0.0 0.0 1.0"/>
  </joint>

  <link name="wheel_left_link">
    <inertial>
       <mass value="2" />
       <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
    </inertial>

    <visual>
      <geometry>
          <cylinder radius="0.07" length="0.05"/>
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.07" length="0.05"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>
      </surface>
      <contact_coefficients mu="1" kp="1e+13" kd="1.0"/>
    </collision>
  </link>

  <joint name="wheel_left_joint" type="continuous">
    <origin xyz="0.1 -0.125 0.0" rpy="1.57 0.0 0.0"/>
    <parent link="base_footprint"/>
    <child link="wheel_left_link"/>
    <axis xyz="0.0 0.0 1.0"/>

  </joint>

  <link name="caster">
    <inertial>
            <mass value="1" />
            <inertia ixx="0.01" ixy="0.0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
    </inertial>

    <visual>
      <geometry>
        <sphere radius=".04" />
      </geometry>
      <material name="white" />
    </visual>

    <collision>
      <geometry>
        <sphere radius=".04" />
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>
  </link>

  <joint name="caster_joint" type="continuous">
    <origin xyz="-0.135 0.0 -0.035" rpy="0.0 0.0 0.0"/>
    <axis xyz="0 0 1" />
    <parent link="base_footprint"/>
    <child link="caster"/>
  </joint>


